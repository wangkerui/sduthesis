\def\sdu@checkoption#1#2{%
  \@for\reserved@a:=#2\do{%
    \csname ifsdu@\reserved@a\endcsname
      \ClassError{sduthesis}%
                 {Please remove `\reserved@a' option when you run #1.}{}
    \fi}}
