  \secdef\@echapter\@sechapter}
\newcommand{\sdu@echaptername}{Chapter}
\def\@echapter[#1]#2{%
  \ifnum \c@secnumdepth >\m@ne
   \if@mainmatter
     \addcontentsline{toe}{chapter}{\sdu@echaptername~\thechapter ~~#1}%
   \else
     \addcontentsline{toe}{chapter}{#1}%
   \fi
  \else
    \addcontentsline{toe}{chapter}{#1}%
  \fi
  \relax\par\nobreak
}
\def\@sechapter#1{%
  \relax\par\nobreak%
}
